{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.min.css*",
        "css/bootstrap.css*",
        "js/bootstrap.bundle.*"
      ]
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "css/all.css",
        "webfonts/*"
      ]
    },
    {
      "library": "marked@4.0.2",
      "destination": "wwwroot/lib/marked/"
    },
    {
      "library": "dompurify@2.3.4",
      "destination": "wwwroot/lib/dompurify/"
    },
    {
      "library": "github-markdown-css@5.1.0",
      "destination": "wwwroot/lib/github-markdown-css/"
    },
    {
      "library": "mathjax@3.2.0",
      "destination": "wwwroot/lib/mathjax/",
      "files": [
        "es5/tex-mml-chtml.js",
        "es5/tex-mml-chtml.min.js",
        "es5/output/chtml/fonts/woff-v2/*.woff"
      ]
    },
    {
      "library": "highlight.js@11.3.1",
      "destination": "wwwroot/lib/highlight.js/",
      "files": [
        "highlight.min.js",
        "styles/default.min.css",
        "languages/python.min.js"
      ]
    },
    {
      "library": "limonte-sweetalert2@11.3.0",
      "destination": "wwwroot/lib/limonte-sweetalert2/"
    },
    {
      "library": "highlightjs-line-numbers.js@2.8.0",
      "destination": "wwwroot/lib/highlightjs-line-numbers.js/"
    }
  ]
}